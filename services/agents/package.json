{
    "name": "@mother-harness/agents",
    "version": "0.1.0",
    "description": "Specialist agents for Mother-Harness orchestration",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
