        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@mother-harness/shared": "workspace:*",
        "nanoid": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "rimraf": "^5.0.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    }
}